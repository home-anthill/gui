    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.51.4",
    "react-icons": "^5.2.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/eslint": "22.1.1",
    "@nx/eslint-plugin": "22.1.1",
    "@nx/js": "22.1.1",
    "@nx/react": "22.1.1",
    "@nx/vite": "22.1.1",
    "@nx/web": "22.1.1",
